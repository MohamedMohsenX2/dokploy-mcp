{
  "name": "dokploy-mcp",
  "version": "2.0.0",
  "description": "Model Context Protocol server for the Dokploy API",
  "main": "dist/mcp-server.js",
  "bin": {
    "dokploy-mcp": "./dist/mcp-server.js"
  },
  "files": [
    "dist",
    ".env.example",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true}); require('fs').rmSync('.tsbuildinfo',{force:true})\"",
    "build": "tsc && node -e \"try{require('fs').chmodSync('dist/mcp-server.js',0o755)}catch(e){}\"",
    "build:clean": "npm run clean && npm run build",
    "start": "node dist/mcp-server.js",
    "dev": "ts-node src/mcp-server.ts",
    "test": "npm run build:clean",
    "prepublishOnly": "npm test",
    "prepack": "npm run build:clean"
  },
  "keywords": [
    "dokploy",
    "mcp",
    "api",
    "middleware",
    "ai"
  ],
  "author": "Mohamed Mohsen",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.2",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.21",
    "@types/node": "^22.15.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
